{
  "version": "1.0.0",
  "mcp_servers": {
    "playwright": {
      "name": "Playwright Browser Automation",
      "description": "Cross-browser automation and testing capabilities",
      "category": "testing",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "enabled": true,
      "auto_start": true,
      "health_check": {
        "enabled": true,
        "interval": 30,
        "timeout": 10,
        "retry_count": 3
      },
      "connection": {
        "host": "localhost",
        "startup_delay": 5
      },
      "capabilities": [
        "browser_automation",
        "screenshot",
        "testing",
        "web_scraping"
      ],
      "priority": 7,
      "tags": ["testing", "automation", "browser"]
    },
    "workout_creator": {
      "name": "Specialized Workout Creator",
      "description": "Advanced workout creation, parsing, and optimization with domain expertise",
      "category": "workout",
      "command": "python",
      "args": ["workout_mcp_server.py"],
      "enabled": true,
      "auto_start": true,
      "health_check": {
        "enabled": true,
        "interval": 20,
        "timeout": 5,
        "retry_count": 2
      },
      "connection": {
        "host": "localhost",
        "port": 8001,
        "startup_delay": 3
      },
      "capabilities": [
        "workout_parsing",
        "interval_creation",
        "tss_calculation",
        "power_zones",
        "complex_intervals"
      ],
      "environment": {
        "WORKOUT_SERVER_PORT": "8001",
        "LOG_LEVEL": "INFO"
      },
      "priority": 1,
      "tags": ["workout", "fitness", "training", "specialized"]
    }
  },
  "global_settings": {
    "max_concurrent_servers": 10,
    "startup_timeout": 30,
    "log_level": "INFO",
    "auto_recovery": true,
    "port_range": {
      "start": 8000,
      "end": 9000
    }
  }
}
